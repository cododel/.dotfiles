[user]
	email = websmithcode@gmail.com
	name = cododel
  signingkey = ssh-rsa 

[gpg]
  format = ssh

[gpg "ssh"]
  program = "/Applications/1Password.app/Contents/MacOS/op-ssh-sign"

[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[pull]
	rebase = false
[commit]
  gpgsign = true
[alias]
  ai-commit = "!f() { ~/.scripts/ai-commit \"$@\"; }; f"
  gitroot = rev-parse --show-toplevel                     

